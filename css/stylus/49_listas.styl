body
	background-color #631888
	font 16px Tahoma
section
	background-color white
	margin 20px auto
	padding 20px
	width 800px
ul, ol
	border 2px dotted #9662AF
	display inline-block
	margin 10px
	vertical-align top
ul.disc li
	list-style-type disc
ul.circle li
	list-style-type circle
	list-style-position inside
ul.square 
	list-style-type square
ol.decimal li 
	list-style-type decimal
ol.roman li 
	list-style-type upper-roman
	list-style-position inside
ol.greek li 
	list-style-type lower-greek
ul.imagen li 
	list-style-image url(../imgs/hola.png)
/*Forma Corta*/
ul.imagen li 
	list-style decimal ouside url(../imgs/hola.png)