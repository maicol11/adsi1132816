doctype
html(lang="en")
  head
    meta(charset="UTF-8")
    title Atributos (Formulario)
  body
  h1 Atributos (Formulario)
  hr
  fieldset
    legend Validad / Autocompletar
    form(method='post')
      label(for='nombre') Nombre:
      input(type='text', id='nombre', name='nombre' required)
      br
      input(type='submit', value='Enviar')
    br
  fieldset
    legend No Validad / No Autocompletar
    form(method='post' autocomplete='off', novalidate)
      label(for='nombre') Nombre:
      input(type='text', id='nombre', name='nombre' required)
      br
      input(type='submit', value='Enviar')
              